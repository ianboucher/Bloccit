source 'https://rubygems.org'


# Bundle edge Rails instead: gem 'rails', github: 'rails/rails'
gem 'rails', '4.2.5'
# Specify the databases for Development and Production environments

gem 'bootstrap-sass'
gem 'sass-rails', '~> 5.0'
gem 'uglifier', '>= 1.3.0'
gem 'coffee-rails', '~> 4.1.0'
gem 'jquery-rails'
gem 'turbolinks'
gem 'jbuilder', '~> 2.0'
gem 'sdoc', '~> 0.4.0', group: :doc
gem 'quiet_assets'
gem 'pry-rails'

group :production do
  gem 'pg'
  gem 'rails_12factor'
end
# Use sqlite3 as the database for Active Record
group :development do
  gem 'sqlite3'
  gem 'web-console', '~> 2.0'
  gem 'spring'
end

group :development, :test do
  gem 'rspec-rails', '~> 3.0'
  gem 'byebug'
end
